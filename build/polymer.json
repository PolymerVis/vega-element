{
  "entrypoint": "index.html",
  "shell": "vega-element.html",
  "fragments": [
    "d3.html",
    "vega-core.html",
    "vega.html",
    "vega-lite.html",
    "vega-tooltip.html",
    "vega-signal.html",
    "vega-data.html",
    "vega-data-stream.html"
  ],
  "sources": [
    "src/**/*",
    "index.html",
    "vega-element.html",
    "d3.html",
    "vega-core.html",
    "vega.html",
    "vega-lite.html",
    "vega-tooltip.html",
    "vega-signal.html",
    "vega-data.html",
    "vega-data-stream.html"
  ],
  "extraDependencies": ["bower_components/webcomponentsjs/*.js"],
  "builds": [
    {
      "name": "es5",
      "js": {
        "minify": true,
        "compile": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": false,
      "addPushManifest": false
    }
  ],
  "lint": {
    "rules": ["polymer-2"]
  }
}
